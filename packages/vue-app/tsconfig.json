{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "sourceMap": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": ["src/*"]
    },
    "types": ["node"]
  },
  "include": ["webpack.config.ts", "./src/*"],
  "extends": "../../tsconfig.json"
}
