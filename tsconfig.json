{
  "compilerOptions": {
    "target": "ES6",
    "strict": true,
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "removeComments": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules"]
}
